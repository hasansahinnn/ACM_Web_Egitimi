@model List<Product>

<h2> table</h2>

<table id="productListTable">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Quantity</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Quantity</td>
        </tr>
    }
</table>